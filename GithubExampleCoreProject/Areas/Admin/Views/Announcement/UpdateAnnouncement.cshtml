@model AnnouncementUpdateDto

@{
    ViewBag.Title = "Duyuru Güncelleme";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Admin/</span> Duyuru Güncelle</h4>
<form method="post">
    <input type="hidden" asp-for="AnnouncementId">
    <label>Duyuru Başlığı</label>
    <input type="text" asp-for="Title" class="form-control">
    <br>
    <label>Duyuru İçeriği</label>
    <input type="text" asp-for="Content" class="form-control">
    <br>
    <button class="btn btn-outline-primary">Duyuruyu Güncelle</button>
</form>